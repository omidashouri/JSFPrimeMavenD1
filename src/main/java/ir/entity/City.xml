<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

        <entity class="ir.entity.City">
                <table name="city" schema="" catalog="sakila"/>
                <attributes>
                        <id name="cityId">
                                <column name="city_id" nullable="false" precision="5"/>
                        </id>
                        <basic name="city">
                                <column name="city" nullable="false" length="50"/>
                        </basic>
                        <basic name="lastUpdate">
                                <column name="last_update" nullable="false" precision="19"/>
                        </basic>
                </attributes>
        </entity>
</entity-mappings>
